<struts>
 
<package name="teacher" namespace="/" extends="struts-default">

<action name="showgroupstudent" class="com.classmarker.actions.Group" method="fetchstudent">
	<result name="success">/group.jsp</result>
	<result name="fail">/login.jsp</result>
	</action> 
	
	<action name="addgroup" class="com.classmarker.actions.Group" method="addgroup">
	<result name="success">/group.jsp</result>
	<result name="fail">/dashboard.jsp</result>
	</action>	
	
	<action name="showgroup" class="com.classmarker.actions.Group" method="fetchgroup">
	<result name="success">/group.jsp</result>
	<result name="fail">/login.jsp</result>
	</action>
	
	<action name="pretestcache" class="com.classmarker.actions.Test" method="fetchbranchsemsubjectgroupchoice">
	<result name="success">/addtest.jsp</result>
	<result name="fail">/login.jsp</result>
	</action> 
	
	<action name="addtest" class="com.classmarker.actions.Test" method="addtest">
	<result name="success">/addtest.jsp</result>
	<result name="fail">/dashboard.jsp</result>
	</action>	
	
</package>


</struts>