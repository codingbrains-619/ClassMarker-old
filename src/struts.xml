<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
	"http://struts.apache.org/dtds/struts-2.5.dtd">
	<struts>
	<constant name="struts.devMode" value="true" />
	<include file="admin.xml"></include>
	<include file="teacher.xml"></include>
	<package name="default" namespace="/" extends="struts-default">
	
	<action name="login" class="com.shubham.actions.Login" method="dologin">
	<result name="success">/dashboard.jsp</result>
	<result name="fail">/login.jsp</result>
	</action>
	
	<action name="showbranchsemester" class="com.shubham.actions.RegisterStudent" method="fetchbranchsem">
	<result name="success">/registerstudent.jsp</result>
	<result name="fail">/login.jsp</result>
	</action>
	
	<action name="registerstudent" class="com.shubham.actions.RegisterStudent" method="addstudent">
	<result name="success">/dashboard.jsp</result>
	<result name="fail">/login.jsp</result>
	</action>
	
	<action name="showbranchsemestersubject" class="com.shubham.actions.RegisterTeacher" method="fetchbranchsemsubject">
	<result name="success">/registerteacher.jsp</result>
	<result name="fail">/login.jsp</result>
	</action>
	
	<action name="registerteacher" class="com.shubham.actions.RegisterTeacher" method="addteacher">
	<result name="success">/dashboard.jsp</result>
	<result name="fail">/login.jsp</result>
	</action>
	
