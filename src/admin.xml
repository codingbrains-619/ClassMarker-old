<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">
 
<struts>
 
<package name="admin" namespace="/" extends="struts-default">
<action name="home" class="com.shubham.actions.Firsttime" method="execute">
	<result name="success">/index.jsp</result>
	<result name="fail">/login.jsp</result>
	</action>
	<action name="firstlogin" class="com.shubham.actions.Firsttime" method="firstlogin">
	<result name="success">/firstpwd.jsp</result>
	<result name="startupfail">/login.jsp</result>
	<result name="partialfail">/index.jsp</result>
	<result name="fail">/index.jsp</result>
	</action>
	<action name="pwdchange" class="com.shubham.actions.Firstupdatepwd" method="firstupdatepwd">
	<result name="success">/login.jsp</result>
	<result name="fail">/firstpwd.jsp</result>
	</action>
	<action name="addsubject" class="com.shubham.actions.Subject" method="addsub">
	<result name="success">/login.jsp</result>
	<result name="fail">/dashboard.jsp</result>
	</action>
	<action name="addsem" class="com.shubham.actions.Semester" method="addsem">
	<result name="success">/dashboard.jsp</result>
	<result name="fail">/login.jsp</result>
	</action>
</package>


</struts>